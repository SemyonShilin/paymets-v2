<%= form_for(:session, url: sessions_path) do |f| %>
  <tr>
    <td><%= f.label :email %></td>
    <td><%= f.text_field :email %></td>
  </tr>
  <tr>
    <td><%= f.label :password, 'Пароль' %></td>
    <td><%= f.password_field :password %></td>
  </tr>
  <tr>
    <td><%= f.submit 'Войти', class: 'btn btn-large btn-primary' %></td>
  </tr>
<% end %>
