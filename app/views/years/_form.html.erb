<%= form_for @year, url: years_path, html: {role: 'form', class: 'form-inline'} do |f| %>
  <%= render 'shared/error_messages', obj: @year %>
  <div class="col-lg-6 col-xs-6 text-right" style="padding-top: 5px;">
    <%= f.text_field :value_year %>
  </div>
  <div class="col-lg-6 col-xs-6 text-left">
    <%= f.submit 'Добавить год', class: 'btn btn-success' %>
  </div>
<% end %>
